<https://ph.org/prod/ontology> a owl:Ontology ;
   rdfs:label "Product Catalog Ontology"@en ;
   rdfs:comment "An ontology for describing product catalogs, categories, brands, and product variants."@en ;
   owl:versionInfo "2.0" .
Prod_Ont:Product a owl:Class .
Prod_Ont:Brand a owl:Class .
Prod_Ont:CategoryTree a owl:Class .
Prod_Ont:Category a owl:Class .
Prod_Ont:ProductVariant a owl:Class .
Prod_Ont:hasparentcategory a owl:ObjectProperty ;
   rdfs:label "hasparentcategory" ;
   rdfs:domain Prod_Ont:Category ;
   rdfs:range Prod_Ont:Category ;
   owl:inverseOf Prod_Ont:hassubcategory .
Prod_Ont:hassubcategory a owl:ObjectProperty ;
   rdfs:label "hassubcategory" ;
   rdfs:domain Prod_Ont:Category ;
   rdfs:range Prod_Ont:Category ;
   owl:inverseOf Prod_Ont:hasparentcategory .
Prod_Ont:type a owl:DatatypeProperty ;
   rdfs:domain Prod_Ont:Product ;
   rdfs:range xsd:string .
Prod_Ont:sku a owl:DatatypeProperty ;
   rdfs:label "sku for Product" ;
